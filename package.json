{
  "name": "Reacthr-starter-kit",
  "version": "2.0.0",
  "description": "learining web packs",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel build-d watcher-d dev-server-d",
    "start-p": "npm-run-all --parallel build-p watcher-p dev-server-p",
    "pre-build": "npm run remove-dist && mkdir Dist",
    "remove-dist": "rimraf ./Dist",
    "build-d": "webpack --config webpack.dev.config.js",
    "watcher-d": "webpack --watch --config webpack.dev.config.js",
    "dev-server-d": "webpack-dev-server --inline  --config webpack.dev.config.js",
    "build-p": "webpack -p --config webpack.prod.config.js",
    "watcher-p": "webpack --watch --config webpack.prod.config.js",
    "dev-server-p": "webpack-dev-server --inline --config webpack.prod.config.js",
    "create": "node createcomponents.js",
    "lint": "eslint --ext .js,.es6,.jsx src/Store*"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "none"
  ],
  "author": "hannad rehman",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-hot": "^1.0.4",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack-dev-server": "^2.5.0",
    "webpack": "^2.6.1",
    "autoprefixer": "^7.1.1",
    "compression-webpack-plugin": "^0.4.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "font-awesome-sass-loader": "^2.0.1",
    "history": "^4.6.3",
    "mkdirp": "^0.5.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3"
  }
}
